{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"strict": false,
		"noEmit": true,
		"skipLibCheck": true
	},
	"include": ["**/*"],
	"exclude": ["node_modules", "dist", "build", ".git"]
}
