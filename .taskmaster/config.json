{
  "models": {
    "main": {
      "provider": "claude-code",
      "modelId": "sonnet",
      "maxTokens": 50000,
      "temperature": 0.2
    },
    "research": {
      "provider": "claude-code",
      "modelId": "opus",
      "maxTokens": 50000,
      "temperature": 0.2
    },
    "fallback": {
      "provider": "claude-code",
      "modelId": "sonnet",
      "maxTokens": 50000,
      "temperature": 0.2
    }
  },
  "global": {
    "logLevel": "info",
    "debug": false,
    "defaultSubtasks": 5,
    "defaultPriority": "medium",
    "projectName": "Taskmaster",
    "ollamaBaseURL": "http://localhost:11434/api",
    "bedrockBaseURL": "https://bedrock.us-east-1.amazonaws.com",
    "userId": "1234567890",
    "azureBaseURL": "https://your-endpoint.azure.com/",
    "defaultTag": "feat-langfuse-integration"
  },
  "features": {
    "worktrees": true
  },
  "costTracking": {
    "enabled": true,
    "alerts": {
      "enabled": true,
      "thresholds": {
        "sessionLimit": 1,
        "taskLimit": 0.5,
        "dailyLimit": 5
      }
    }
  },
  "observability": {
    "langfuse": {
      "enabled": true,
      "baseUrl": "https://cloud.langfuse.com",
      "debug": false,
      "samplingRate": 1,
      "promptResponseLogging": true,
      "batchSize": 10,
      "redactionPatterns": []
    }
  }
}
